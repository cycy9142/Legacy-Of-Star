        <arguments>
          <argument>
            <kind>1</kind>
            <string>///공격 관련 

//구르기  
if( keyboard_check_pressed(ord('C')) &amp;&amp; g.game_spd_val &gt; 0 )
{
    //공중 검사 
    if( ground_on )
    {
        //딜레이 검사
        if( roll_delay_val &lt;= 0 )
        {
            //모션 변경
            spr_t_name = 0;
            spr_t_index = 0;
            spr_b_name = spr_joe_a_roll;
            spr_b_index = 0;
            //딜레이 설정
            roll_delay_val = roll_delay;
            roll_dir = look_dir;
            //스킬 취소 
            if( skill_a_on )
            {
                skill_a_on = false;
                skill_a_cool_val = skill_a_cool_time;
            }
            if( skill_s_len_val &gt;= 0 )
            {
                skill_s_len_val = -1;
                move_h_spd = sign(skill_s_dir) * -skill_s_back;
                move_v_spd = -skill_s_back;
                skill_s_ef_inst.destroy = true;
                skill_s_ef_inst = -1;
            }
        }
    }
}


//총 쏘기
if( keyboard_check_pressed(ord('X')) &amp;&amp; g.game_spd_val &gt; 0 )
{
    //스킬 사용 검사
    if( !skill_a_on &amp;&amp; !skill_s_on &amp;&amp; !skill_d_on &amp;&amp; !skill_f_on)
    {
        //기술 사용 검사 
        if( spr_b_name != spr_joe_a_roll and spr_b_name != spr_joe_a_skill_02)
        {
            //딜레이 검사
            if( shot_delay_val &lt;= 0 )
            {
                if( shot_bullet &gt; 0 )
                {
                    //총알 감소
                    shot_bullet -= 1;
                    shot_delay_val = shot_delay;
                    //모션 변경
                    spr_t_name = spr_joe_t_shot;
                    spr_t_index = 0;
                    if( spr_b_name == spr_joe_a_idle ) 
                    {
                        spr_b_name = spr_joe_b_idle;
                        spr_b_index = 0;
                    }
                    //소리 재생 
                    audio_play_sound(sound0_pistol_shoot, 0, false);
                    //총알 발사 
                    inst = instance_create(x+(60*look_dir), y-55, obj_hero_bullet);
                    inst.direction = 90 + (-90*look_dir);
                    inst.dmg = hero_dmg;
                    inst = instance_create(x+(60*look_dir), y-55, obj_bullet_ef);
                    inst.image_angle = 90 + (-90*look_dir);
                    //잔여 총알 검사 
                    if( shot_bullet == 0 )
                    {
                        shot_reload_delay_val = shot_reload_delay;
                    }
                }
            }
        }
    }
}

if( shot_delay_val &gt; 0 ) shot_delay_val -= g.game_spd_val;

if( shot_reload_delay_val != -10 )
{
    if( shot_reload_delay_val &gt; 0 ) {
        shot_reload_delay_val -= g.game_spd_val;
    }else{
        shot_reload_delay_val = -10;
        shot_bullet = shot_bullet_max;
    }

}


if( hit_dmg &gt; 0 )
{
    if( inv_val &gt; 0 || roll_delay_val &gt; 0 || skill_a_on || skill_s_inv &gt; 0)
    {
        if( skill_a_on )
        {
            if( skill_a_inv &gt; 0 )
            {
                skill_a_inv -= g.game_spd_val;
            }
            else
            {
                inst = instance_create(x, mean(bbox_top, bbox_bottom), obj_dmg);
                inst.h_spd = inst.h_spd * sign(knock_power);
                inst.dmg = "BLOCK";
                instance_create( x + look_dir * 60, 
                                 mean(bbox_top, bbox_bottom), obj_block_ef);
                skill_a_inv = inv_time * 0.5;
            }
        }
        if( skill_s_inv &gt; 0 )
        {
            skill_s_inv -= g.game_spd_val;
        }
        hit_dmg = 0;
        knock_power = 0;
    }
    else
    {
        inv_val = inv_time;
        hero_hp -= hit_dmg;
        
        move_h_spd += knock_power;
        move_v_spd -= abs(knock_power);
        
        inst = instance_create(x, mean(bbox_top, bbox_bottom), obj_dmg);
        inst.h_spd = inst.h_spd * sign(knock_power);
        inst.dmg = hit_dmg;
        inst.target_is_enemy = false;
        
        hit_dmg = 0;
        knock_power = 0;
        
        //스킬 시전 취소 
        skill_s_len_val = -1;
        if( instance_exists( skill_s_ef_inst ) )
        {
            skill_s_ef_inst.destroy = true;
            skill_s_ef_inst = -1;
        }
        
    }
}

if( inv_val &gt; 0 ) {
    inv_val -= g.game_spd_val;
}


if( hero_hp &lt;= 0 )
{
    inst = instance_create(x, y, obj_joe_die);
    inst.move_h_spd = move_h_spd;
    inst.move_v_spd = move_v_spd;
    inst.image_xscale = image_xscale;
    instance_destroy();
}
</string>
          </argument>
        </arguments>
      </action>
      <action>
        <libid>1</libid>
