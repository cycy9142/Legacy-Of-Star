        <arguments>
          <argument>
            <kind>1</kind>
            <string>///스킬 관련
//A 스킬 관련
if( skill_a_cool_val &lt;= 0 )
{
    if( keyboard_check_pressed(ord('A')) &amp;&amp; ground_on &amp;&amp; g.game_spd_val &gt; 0 &amp;&amp; skill_s_len_val == -1)
    {
        if( skill_a_on ) 
        {
            skill_a_on = false;
            skill_a_cool_val = skill_a_cool_time;
        }
        else
        {
            skill_a_on = true;
            //모션 변경
            spr_t_name = 0;
            spr_t_index = 0;
            spr_b_name = spr_joe_a_skill_01;
            spr_b_index = 0;
        }
    }
}
else
{
    skill_a_cool_val -= g.game_spd_val;
}

///S 스킬 관련
if( skill_s_cool_val &lt;= 0 )
{
    if( keyboard_check_pressed(ord('S')) &amp;&amp; ground_on &amp;&amp; g.game_spd_val &gt; 0 )
    {
        skill_s_cool_val = skill_s_cool_time;
        skill_s_len_val = 0;
        skill_s_dir = look_dir;
        skill_s_start_x = x;
        skill_s_dust = 0;
        //모션 변경
        spr_t_name = 0;
        spr_t_index = 0;
        spr_b_name = spr_joe_a_skill_02;
        spr_b_index = 0;
    }
}
else
{
    skill_s_cool_val -= g.game_spd_val;
}

if( spr_b_name == spr_joe_a_skill_02 )
{
    if( spr_b_index &gt;= 30 )
    {
        if( skill_s_len_val != -1 )
        {
            if( skill_s_len_val &gt; skill_s_len )
            {
                skill_s_len_val = -1;
                move_h_spd = sign(skill_s_dir) * -skill_s_back;
                move_v_spd = -skill_s_back;
                skill_s_ef_inst.destroy = true;
                skill_s_ef_inst = -1;
                instance_create( x + (sign(skill_s_dir) * (bbox_right-bbox_left) * 0.5), 
                                 y, obj_skill_s_attack);
            }
            else
            {
                if( ground_on )
                {
                    if( !instance_exists(obj_boost) )
                    {
                        skill_s_ef_inst = instance_create(x, mean(bbox_top, bbox_bottom), obj_boost);
                        skill_s_ef_inst.image_xscale = sign(skill_s_dir);
                    }   
                    
                    if( !place_free( x + skill_s_spd * g.game_spd_val, y) or
                        collision_point( x + (sign(skill_s_dir) * (bbox_right-bbox_left) * 0.5), 
                                    bbox_bottom+2, obj_col_parant, true, true) == noone )
                    {
                        skill_s_len_val = -1;
                        move_h_spd = sign(skill_s_dir) * -skill_s_back;
                        move_v_spd = -skill_s_back;
                        skill_s_ef_inst.destroy = true;
                        skill_s_ef_inst = -1;
                    }
                    else
                    {
                        x += skill_s_spd * sign(skill_s_dir) * g.game_spd_val;
                        skill_s_ef_inst.x = x;
                        
                        if( abs(skill_s_start_x - x) &gt; skill_s_dust)
                        {
                            inst_dust = instance_create(x, y, obj_dust);
                            inst_dust.image_xscale = sign(skill_s_dir);
                            skill_s_dust += 100;
                        }
                        
                        temp_col = collision_line( x + (sign(skill_s_dir) * (bbox_right-bbox_left) * 0.5),
                                                   bbox_top,
                                                   x + (sign(skill_s_dir) * (bbox_right-bbox_left) * 0.5),
                                                   bbox_bottom, obj_enemy, false, true);
                        while (temp_col != noone)
                        {
                            temp_pre_x = temp_col.x;
                            if( sign(skill_s_dir) == 1 ) temp_col.x = bbox_right  + (temp_col.x - temp_col.bbox_left + 1);
                                else temp_col.x = bbox_left - (temp_col.bbox_right - temp_col.x + 1);
                                
                            if( collision_line( temp_col.x + (sign(skill_s_dir) 
                                                * (temp_col.bbox_right-temp_col.bbox_left) * 0.5), 
                                                temp_col.bbox_top, 
                                                temp_col.x + (sign(skill_s_dir) 
                                                * (temp_col.bbox_right-temp_col.bbox_left) * 0.5), 
                                                temp_col.bbox_bottom, 
                                                obj_col_parant, true, true) != noone or
                                collision_point( temp_col.x + (sign(skill_s_dir) 
                                                 * (temp_col.bbox_right-temp_col.bbox_left) * 0.5), 
                                                 bbox_bottom+2, obj_col_parant, true, true) == noone )
                            {
                                temp_col.x = temp_pre_x;
                                skill_s_len_val = -1;
                                move_h_spd = sign(skill_s_dir) * -skill_s_back;
                                move_v_spd = -skill_s_back;
                                skill_s_ef_inst.destroy = true;
                                skill_s_ef_inst = -1;
                                inst_atk = instance_create( x + (sign(skill_s_dir) * (bbox_right-bbox_left) * 0.5), 
                                                 y, obj_skill_s_attack);
                                inst_atk.dmg = skill_s_dmg;
                                break;
                            }
                            else
                            {
                                temp_col.det_disturb = inv_time * 0.5;
                            }
                            
                            
                            temp_col = collision_line( x + (sign(skill_s_dir) * (bbox_right-bbox_left) * 0.5),
                                                       bbox_top,
                                                       x + (sign(skill_s_dir) * (bbox_right-bbox_left) * 0.5),
                                                       bbox_bottom, obj_enemy, false, true);
                        };
                        
                        
                        skill_s_len_val += skill_s_spd * g.game_spd_val;
                        spr_b_index = min(spr_b_index, 60);
                        look_dir = skill_s_dir;
                        skill_s_inv = inv_time * 0.5;
                    }
                }
                else
                {
                    skill_s_len_val = -1;
                    move_h_spd = sign(skill_s_dir) * -skill_s_back;
                    move_v_spd = -skill_s_back;
                    skill_s_ef_inst.destroy = true;
                    skill_s_ef_inst = -1;
                    instance_create( x + (sign(skill_s_dir) * (bbox_right-bbox_left) * 0.5), 
                                     y, obj_skill_s_attack);
                }
            }
        }
    }
}


///D 스킬 관련
if( skill_d_cool_val &lt;= 0 )
{
